{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.2.0/schema.json",
  "global": {
    "fabricVersion": "2.5.12",
    "tls": true
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "root.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 1
        }
      ]
    },
    {
      "organization": {
        "name": "Org1",
        "domain": "org1.com"
      },
      "peer": {
        "instances": 2,
        "anchorPeerInstances": 2
      }
    },
    {
      "organization": {
        "name": "Org2",
        "domain": "org2.com"
      },
      "peer": {
        "instances": 2,
        "anchorPeerInstances": 2
      }
    }
  ],
  "channels": [
    {
      "name": "my-channel1",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0",
            "peer1"
          ]
        },
        {
          "name": "Org2",
          "peers": [
            "peer0",
            "peer1"
          ]
        }
      ]
    },
    {
      "name": "my-channel2",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer1"
          ]
        },
        {
          "name": "Org2",
          "peers": [
            "peer1"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "chaincode1",
      "version": "0.0.1",
      "lang": "node",
      "channel": "my-channel1",
      "endorsement": "OR('Org1MSP.member', 'Org2MSP.member')",
      "directory": "./chaincode-kv-node",
      "privateData": [
        {
          "name": "private-collection",
          "orgNames": [
            "Org1",
            "Org2"
          ]
        }
      ]
    }
  ],
  "hooks": {
    "postGenerate": "perl -i -pe 's/docker-compose/docker compose/g' \"./fablo-target/fabric-docker/commands-generated.sh\" && perl -i -pe 's/docker-compose/docker compose/g' \"./fablo-target/fabric-docker/snapshot-scripts.sh\""
  }
}
